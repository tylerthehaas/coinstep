{
  "extends": ["airbnb", "prettier", "prettier/flowtype"],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "plugins": ["prettier", "flowtype"],
  "globals": {
    "document": false,
    "FormData": false,
    "window": false
  },
  "rules": {
    "react/forbid-prop-types": 2,
    "no-use-before-define": "off",
    "no-console": ["error", { "allow": ["error", "info"] }]
  }
}
